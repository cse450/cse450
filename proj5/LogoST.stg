group LogoST;

Output(instructions)::=<<
THIS IS THE START OF THE OUTPUT

<instructions; separator="\n">

THIS IS THE END OF THE OUTPUT
>>

assign(expression, varNum) ::= <<
<expression; separator="\n">
istore <varNum>
>>

var(a) ::= <<
var <a>
>>

add(a, b) ::= <<
<a>
<b>
iadd
>>

sub(a, b) ::= <<
<a>
<b>
isub
>>

mul(a, b) ::= <<
<a>
<b>
imul
>>

div(a, b) ::= <<
<a>
<b>
idiv
>>

int(i) ::= <<
ldc <i>
>>
