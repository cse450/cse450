group LogoST;

Output(instructions)::=<<
THIS IS THE START OF THE OUTPUT

<instructions; separator="\n">

THIS IS THE END OF THE OUTPUT
>>

assign(a, b) ::= <<
<a> = <b>
>>

var(a) ::= <<
var <a>
>>

add(a, b) ::= <<
<a> + <b>
>>

sub(a, b) ::= <<
<a> - <b>
>>

mul(a, b) ::= <<
<a> * <b>
>>

div(a, b) ::= <<
<a> / <b>
>>

int(a) ::= <<
int <a>
>>
